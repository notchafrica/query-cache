{
    "name": "chapdel/query-cache",
    "description": "Adding cache on your Laravel Eloquent queries' results is now a breeze.",
    "keywords": [
        "laravel",
        "caching",
        "eloquent",
        "remember",
        "query",
        "sql"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-co/laravel-eloquent-query-cache",
    "authors": [
        {
            "name": "Alex Renoki",
            "email": "alex@renoki.org",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Rennokki\\QueryCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rennokki\\QueryCache\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
